# Ignore node_modules in both backend and frontend
frontend/node_modules/
backend/node_modules/

# Ignore build artifacts (you may customize this based on your build tools)
frontend/dist/
frontend/build/
backend/dist/

# Ignore sensitive files such as environment files (which are handled inside frontend and backend .gitignore)
frontend/.env
backend/.env

# Ignore logs (for both backend and frontend)
frontend/logs/
backend/logs/

# IDE specific
.vscode/
.idea/

# Ignore operating system specific files
.DS_Store
Thumbs.db
